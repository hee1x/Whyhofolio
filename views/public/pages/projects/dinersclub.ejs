    console.log(navbarHeight);
    contactBody = document.querySelector("#contactBody")
    height = document.documentElement.clientHeight - navbarHeight + "px"
    console.log(document.documentElement.clientHeight + " " + navbarHeight + " " + height)
    contactBody.style.height = height
